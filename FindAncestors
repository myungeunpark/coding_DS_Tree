public class Ancestor {

	public class Node{
		
		private Node left;
		private Node right;
		private int val;
		
		public Node(int val ){
			
			this.val = val;
			this.left = null;
			this.right = null;
		}
	}
	
		private boolean findAncestors(Node root, int x, List<Node> list){
        
        if(root == null)
            return false;
            
        if(root.val == x){
            return true;
        }
        
        list.add(root);
        
        if(findAncestors(root.left, x, list))
            return true;
        
        if(findAncestors(root.right, x, list))
            return true;
            
        list.remove(list.size()-1);
        return false;
    }
	}
}
